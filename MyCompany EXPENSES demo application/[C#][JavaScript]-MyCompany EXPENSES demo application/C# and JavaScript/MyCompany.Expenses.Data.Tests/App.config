  <appSettings>
    <add key="Microsoft.ServiceBus.ConnectionString" value="Endpoint=sb://xx.servicebus.windows.net;SharedSecretIssuer=owner;SharedSecretValue=xxx" />
    <add key="ida:Tenant" value="[tenantname].onmicrosoft.com" />
  </appSettings>
  <connectionStrings>
    <add name="MyCompany.Expenses" providerName="System.Data.SqlClient" connectionString="Server=(localdb)\v11.0;Database=MyCompany.Expenses.Data.Test;Trusted_Connection=true;Integrated Security=True" />
  </connectionStrings>
  <system.diagnostics>
